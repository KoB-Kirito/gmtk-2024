save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={}
layout/style_list=["res://story/storystyle123.tres"]
layout/default_style="res://story/storystyle123.tres"
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/SimpleTerrain/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/terraincrafter/plugin.cfg")

